<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Function #<%= @function.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/functions" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/functions/<%= @function.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit function
        </a>
      </div>

      <div class="col">
        <a href="/delete_function/<%= @function.id %>" class="btn btn-block btn-outline-secondary">
          Delete function
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Function name
      </dt>
      <dd>
        <%= @function.function_name %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@function.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@function.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "function_templates/function_templates" %>
  </div>
</div>
